                    {
                        "type": 1,
                        "content": {
                            "json": "Avoid placing Traffic Manager behind Front Door. Check [this link](https://learn.microsoft.com/azure/frontdoor/best-practices#avoid-combining-traffic-manager-and-front-door) for further information."
                        },
                        "name": "querytext1"
